-- Bağlantıyı kopyalama fonksiyonu
local function copyToClipboard(text)
    if setclipboard then
        setclipboard(text)
        print("Bağlantı panoya kopyalandı!")
    else
        warn("setclipboard fonksiyonu desteklenmiyor.")
    end
end

-- Kopyalanacak Discord bağlantısı
local discordLink = "https://discord.com/invite/NfEj3JhBAY"

-- Bağlantıyı panoya kopyala
copyToClipboard(discordLink)

                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end
                    }
                }
            })
        end
    })
    
    
    
